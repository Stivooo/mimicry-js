{
  "name": "mimicry-js",
  "version": "0.0.1",
  "description": "Library for generating mocks for tests",
  "repository": "git@github.com:Stivooo/mimicry-js.git",
  "author": "Vladimir Rybin <vladimir.rybin.98@gmail.com>",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "testing",
    "mocks"
  ],
  "files": [
    "build/"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist",
    "prepare": "yarn clean && yarn build",
    "test": "jest -c jest.config.mjs",
    "test-with-coverage": "jest -c jest.config.mjs --coverage",
    "lint": "eslint"
  },
  "devDependencies": {
    "@eslint/js": "9.21.0",
    "@rollup/plugin-terser": "0.4.4",
    "@types/jest": "29.5.14",
    "@types/node": "18",
    "eslint": "9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-prettier": "5.2.3",
    "jest": "29.7.0",
    "prettier": "3.5.2",
    "rollup": "4.34.8",
    "rollup-plugin-typescript2": "0.36.0",
    "ts-jest": "29.2.6",
    "tslib": "2.8.1",
    "typescript": "5.7.3",
    "typescript-eslint": "8.25.0"
  },
  "dependencies": {}
}
